package pertemuanke38;
public class PertemuanKe38 {
    public static void main(String[] args) {
        // Deklarasi variabel int
        int nilai1 = 10;
        int nilai2 = 20;
        
        // Operasi menggunakan variabel int
        int hasilPenjumlahan = nilai1 + nilai2;
        
        // Menampilkan hasil
        System.out.println("Hasil Penjumlahan: " + hasilPenjumlahan);
    }
}
