package pertemuanke37;
public class PertemuanKe37 {
    public static void main(String[] args) {
        int nilai = 10;
        
        if (nilai >= 20) {
            System.out.println("A");
        } else if (nilai >= 60) {
            System.out.println("B");
        } else if (nilai >= 70) {
            System.out.println("C");
        } else {
            System.out.println("D");
        }
    }
}
